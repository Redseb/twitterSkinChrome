{
"manifest_version": 2,
    "name": "Twitter Purple",
    "description": "Changes the color scheme of Twitter!",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://twitter.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}